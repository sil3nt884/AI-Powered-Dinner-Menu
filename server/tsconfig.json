{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "downlevelIteration": true,
    "target": "esnext",
    "incremental": true

  },
  "exclude": ["node_modules", "../web/dinner/*"],
    "include": [ "**/*.ts"],

}

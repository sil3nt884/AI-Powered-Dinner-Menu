{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": false,
    "outDir": "./dist",
    "rootDir": "./",
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "isolatedModules": true,
    "downlevelIteration": true,
    "incremental": true

  },
  "include": ["./routes/*.ts", "api.ts", "redis.ts", "pg.ts", "HtmlPasrer.ts" ],
  "exclude": ["node_modules", "../web/dinner/**", "dist"],
}
